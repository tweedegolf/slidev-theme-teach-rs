{
  "name": "slidev-theme-teach-rs",
  "author": "Henk Oordt <hd@oordt.dev>",
  "version": "0.0.1",
  "keywords": [
    "slidev-theme",
    "slidev",
    "teach-rs",
    "101-rs",
    "Rust 101"
  ],
  "engines": {
    "node": ">=18.0.0",
    "slidev": ">=0.19.3"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/theme-default": "^0.21.2",
    "@slidev/types": "^0.43.7",
    "codemirror-theme-vars": "^0.1.2",
    "prism-theme-vars": "^0.2.4",
    "theme-vitesse": "^0.7.3"
  },
  "devDependencies": {
    "@slidev/cli": "^0.43.7"
  },
  "//": "Learn more: https://sli.dev/themes/write-a-theme.html",
  "slidev": {
    "colorSchema": "both",
    "highlighter": "shiki",
    "defaults": {
      "fonts": {
        "sans": "Nunito Sans",
        "mono": "Fira Mono"
      },
      "class": "text-center",
      "lineNumbers": "true",
      "drawings": {
        "persist": false
      }
    }
  },
  "license": "(MIT OR Apache-2.0)"
}
